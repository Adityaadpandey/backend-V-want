{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "./api/auth",
            "dest": "./index.js",
            "methods": ["GET","POST","PUT","DELETE"]
